<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jaycuffs.drunkchecker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Dashboard"
            android:label="@string/title_activity_dashboard" />
        <activity
            android:name=".checker_dashboard"
            android:label="@string/title_activity_checker_dashboard"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jaycuffs.drunkchecker.MainActivity" />
        </activity>
        <activity
            android:name=".question"
            android:label="@string/title_activity_question"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jaycuffs.drunkchecker.MainActivity" />
        </activity>
        <activity
            android:name=".questions"
            android:label="@string/title_activity_questions"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jaycuffs.drunkchecker.MainActivity" />
        </activity>
        <activity
            android:name=".Score"
            android:label="@string/title_activity_score"
            android:parentActivityName=".questions">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.jaycuffs.drunkchecker.questions" />
        </activity>
    </application>

</manifest>